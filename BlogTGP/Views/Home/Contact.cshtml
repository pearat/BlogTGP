@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="space-60"></div>
<h5 class="margin-b-30">Leave a reply</h5>
<form id  ="contact-form" actiont="@Url.Action("Contact","Home")" 
      method="post" novalidate
      class="form-horizontal comment-form">
    <div class="form-group">
        <label for="name1" class="col-sm-2 control-label">Name</label>
        <div class="col-sm-10">
            <input id="contact-name" name="Name" type="text"
                   class="form-control requiredField">
        </div>
    </div>
    <div class="form-group">
        <label for="email1" class="col-sm-2 control-label">Email</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="email1" name="Name">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-10 col-sm-offset-2">
            <textarea class="form-control" rows="3" id="comment" name="Subject"></textarea>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button name="submit" type="submit" id="submit" data-error-message="Error!" 
                    data-sending-message="Sending..."
                    data-ok-message="Message"value="true"
                    class="btn btn-dark btn-lg">Submit</button>
        </div>
    </div>
    <input type="hidden" name="submitted" id="submitted" value="true"/>
</form>
<div class="space-80"></div>